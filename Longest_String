import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String input1 = "";
        String longest = "";
        String end = "END";
        while (!input1.equals("END")) {
            input1 = scanner.nextLine();
            if (input1.length() >= longest.length()){
                longest = input1;
            }
        }
        System.out.println(longest);
    }
}
